        "esbuild": "^0.27.2",
        "jest": "^30.2.0",
        "jest-junit": "^16.0.0",
        "msw": "^2.12.7",
        "node-schedule": "^2.1.1",
        "tiktok-live-connector": "^2.1.0",
        "ts-jest": "^29.4.6",
        "ts-jest-resolver": "^2.0.1",
        "ts-node": "^10.9.2",
        "typescript": "^5.9.3"
    },
